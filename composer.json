{
    "name": "sinsquare/silex-yaml-loader",
    "description": "YAML config loader for Silex",
    "type": "library",
    "require": {
        "php": "~5.5|~7.0",
        "silex/silex": "^2.0",
        "symfony/dependency-injection": "~2.8|^3.0",
        "symfony/yaml": "~2.8|^3.0",
        "symfony/config": "~2.8|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "Sins\\Config\\": "src/Sins/Config/" }
    },
    "autoload-dev": {
        "psr-4": { "Sins\\Tests\\": "tests/Sins/Tests/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}